{
  "name": "@wazza99/use-sortable",
  "version": "1.0.3",
  "license": "MIT",
  "author": "Leonard Ikeh",
  "main": "dist/index.js",
  "module": "dist/use-sortable.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/leoikeh99/use-sortable#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/leoikeh99/use-sortable"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests"
  },
  "keywords": [
    "react",
    "hooks",
    "drag-and-drop",
    "sorting",
    "sortable"
  ],
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/use-sortable.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-sortable.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@tsconfig/recommended": "^1.0.7",
    "@tsconfig/vite-react": "^3.0.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "dts-cli": "^2.0.5",
    "husky": "^9.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^11.1.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "nanoid": "^5.0.7"
  }
}
